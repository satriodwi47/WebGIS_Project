    };

// Fungsi untuk mendapatkan warna berdasarkan nama kabupaten
function getColor(kabupaten) {
    return colorMap[kabupaten] || "#" + Math.floor(Math.random() * 16777215).toString(16); // Warna acak jika kabupaten tidak ada di colorMap
}
// Fungsi showAdminMap dengan warna dinamis
function showAdminMap() {
    console.log("showAdminMap called");
    if (currentLayer) {
        console.log("Removing existing layer");
        map.removeLayer(currentLayer);
    }
    console.log("Adding GeoJSON layer");
    currentLayer = L.geoJSON(kabupatenGeoJSON, {
        style: function (feature) {
            return {
                color: getColor(feature.properties.Kabupaten), // Warna garis berdasarkan kabupaten
                weight: 3,
                fillColor: getColor(feature.properties.Kabupaten), // Warna isi berdasarkan kabupaten
                fillOpacity: 0.9,
                dashArray: '5, 5'
            };
        },
        onEachFeature: function (feature, layer) {
            var popupContent = `
                <div class="p-3" style="min-width: 350px; max-width: 400px; font-family: Arial, sans-serif; max-height: 400px; overflow-y: auto;">
                    <h4 class="mb-3 text-primary">${feature.properties.Kabupaten}</h4>
                    <div class="accordion" id="accordionPopup">
                        <div class="accordion-item">
                            <h2 class="accordion-header" id="headingBasic">
                                <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseBasic" aria-expanded="true" aria-controls="collapseBasic">
                                    Informasi Dasar
                                </button>
                            </h2>
                            <div id="collapseBasic" class="accordion-collapse collapse show" aria-labelledby="headingBasic">
                                <div class="accordion-body">
                                    <table class="table table-sm table-bordered">
                                        <tr><th class="bg-light">Provinsi</th><td>${feature.properties.Provinsi}</td></tr>
                                        <tr><th class="bg-light">Luas</th><td>${feature.properties.Luas_Km2} kmÂ²</td></tr>
                                        <tr><th class="bg-light">Populasi</th><td>${feature.properties.Populasi}</td></tr>
                                        <tr><th class="bg-light">Kecamatan</th><td>${feature.properties.Kecamatan}</td></tr>
                                        <tr><th class="bg-light">Kelurahan</th><td>${feature.properties.Kelurahan}</td></tr>
                                        <tr><th class="bg-light">Desa</th><td>${feature.properties.Desa}</td></tr>
                                        <tr><th class="bg-light">RW</th><td>${feature.properties.RW}</td></tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="accordion-item">
                            <h2 class="accordion-header" id="headingEnvironment">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseEnvironment" aria-expanded="false" aria-controls="collapseEnvironment">
                                    Informasi Lingkungan
                                </button>
                            </h2>
                            <div id="collapseEnvironment" class="accordion-collapse collapse" aria-labelledby="headingEnvironment">
                                <div class="accordion-body">
                                    <table class="table table-sm table-bordered">
                                        <tr><th class="bg-light">Isu Lingkungan</th><td>${feature.properties.Isu_LH.replace(/\n/g, '<br>')}</td></tr>
                                        <tr><th class="bg-light">Laboratorium Lingkungan</th><td>${feature.properties.Lab_Ling}</td></tr>
                                        <tr><th class="bg-light">Perling</th><td>${feature.properties.Perling.replace(/\n/g, '<br>')}</td></tr>
                                        <tr><th class="bg-light">Proklim</th><td>${feature.properties.Proklim}</td></tr>
                                        <tr><th class="bg-light">Roadmap</th><td>${feature.properties.Roadmap}</td></tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="accordion-item">
                            <h2 class="accordion-header" id="headingWaste">
                                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseWaste" aria-expanded="false" aria-controls="collapseWaste">
                                    Pengelolaan Sampah
                                </button>
                            </h2>
                            <div id="collapseWaste" class="accordion-collapse collapse" aria-labelledby="headingWaste">
                                <div class="accordion-body">
                                    <table class="table table-sm table-bordered">
                                        <tr><th class=" hous-light">Timbulan Sampah</th><td>${feature.properties.Timbulan_S.replace(/\n/g, '<br>')}</td></tr>
                                        <tr><th class="bg-light">Sampah Kelola</th><td>${feature.properties.Sampah_Kel.replace(/\n/g, '<br>')}</td></tr>
                                        <tr><th class="bg-light">Fasilitas</th><td>${feature.properties.Fasilitas_.replace(/\n/g, '<br>')}</td></tr>
                                        <tr><th class="bg-light">Komposisi Sampah</th><td>${feature.properties.Komposisi_.replace(/\n/g, '<br>')}</td></tr>
                                        <tr><th class="bg-light">Sumber Sampah</th><td>${feature.properties.Sumber_Sam.replace(/\n/g, '<br>')}</td></tr>
                                        <tr><th class="bg-light">IKLH</th><td>${feature.properties.IKLH.replace(/\n/g, '<br>')}</td></tr>
                                        <tr><th class="bg-light">Limbah B3</th><td>${feature.properties.B3.replace(/\n/g, '<br>')}</td></tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                  `;
                    layer.bindPopup(popupContent, { maxWidth: 400 });
                }
            }).addTo(map);
            console.log("Layer added:", currentLayer);
            try {
                map.fitBounds(currentLayer.getBounds());
                console.log("Bounds set successfully");
            } catch (e) {
                console.error("Error fitting bounds:", e);
                map.setView([-7.0, 113.0], 10);
            }
            hideTable();
            applySearchFilter();
        }


// Fungsi untuk menampilkan layer fasilitas
        function showLayer(type) {
            if (currentLayer) {
                map.removeLayer(currentLayer);
            }
            currentLayer = L.layerGroup();
            facilitiesGeoJSON[type].forEach(function (item) {
                var popupContent = `
                    <div class="p-3" style="min-width: 200px; max-width: 300px; font-family: Arial, sans-serif; max-height: 300px; overflow-y: auto;">
                        <h5 class="mb-2 text-primary">${item.name}</h5>
                        <table class="table table-sm table-bordered">
                            <tr><th class="bg-light">Tipe</th><td>${type}</td></tr>
                            <tr><th class="bg-light">Kategori</th><td>${item.description}</td></tr>
                             <tr><th class="bg-light">Provinsi</th><td>${item.provinsi || 'N/A'}</td></tr>
                            <tr><th class="bg-light">Kabupaten</th><td>${item.kabupaten}</td></tr>
                            <tr><th class="bg-light">Kecamatan</th><td>${item.kecamatan || 'N/A'}</td></tr>
                            <tr><th class="bg-light">Latitude</th><td>${item.lat}</td></tr>
                            <tr><th class="bg-light">Longitude</th><td>${item.lon}</td></tr>
                            <tr><th class="bg-light">Kapasitas(m3/hari)</th><td>${item.kapasitas || 'N/A'}</td></tr>
                            <tr><th class="bg-light">Luas Total(ha)</th><td>${item.luas_total || 'N/A'}</td></tr>
                            <tr><th class="bg-light">Luas Unit(ha)</th><td>${item.luas_unit || 'N/A'}</td></tr>
                            <tr><th class="bg-light">Kapasitas(m3/hari)</th><td>${item.kapasitas1 || 'N/A'}</td></tr>
                        </table>
                    </div>
                `;
                L.marker([item.lat, item.lon], { kabupaten: item.kabupaten })
                    .bindPopup(popupContent, { maxWidth: 300 })
                    .addTo(currentLayer);
            });
            currentLayer.addTo(map);
            try {
                map.fitBounds(currentLayer.getBounds());
            } catch (e) {
                console.error("Error fitting bounds:", e);
                map.setView([-7.0, 113.0], 10);
            }
            updateTable(type);
            applySearchFilter();
        }

        // Fungsi untuk menampilkan peta ekoregion
function showEcoregionMap() {
    if (currentLayer) {
        map.removeLayer(currentLayer);
    }
    currentLayer = L.geoJSON(ecoregionGeoJSON, {
        style: function (feature) {
            return {
                color: feature.properties.kba_250.includes('Hutan') ? '#1b5e20' : '#f9a825',
                weight: 2,
                fillOpacity: 0.5
            };
        },
        onEachFeature: function (feature, layer) {
            var popupContent = `
                <div class="p-3" style="min-width: 200px; max-width: 300px; font-family: Arial, sans-serif; max-height: 300px; overflow-y: auto;">
                    <h5 class="mb-2 text-primary">${feature.properties.kba_250}</h5>
                    <table class="table table-sm table-bordered">
                        <tr><th class="bg-light">Kode KBA</th><td>${feature.properties.kode_kba}</td></tr>
                        <tr><th class="bg-light">Klasifikasi</th><td>${feature.properties.kba_250}</td></tr>
                        <tr><th class="bg-light">Kabupaten/Kota</th><td>${feature.properties.WADMKK}</td></tr>
                        <tr><th class="bg-light">Provinsi</th><td>${feature.properties.WADMPR}</td></tr>
                        <tr><th class="bg-light">Panjang (m)</th><td>${feature.properties.Shape_Leng.toFixed(2)}</td></tr>
                        <tr><th class="bg-light">Luas (mÂ²)</th><td>${feature.properties.Shape_Area.toFixed(4)}</td></tr>
                    </table>
                </div>
            `;
            layer.bindPopup(popupContent, { maxWidth: 300 });
        }
    }).addTo(map);
    try {
        map.fitBounds(currentLayer.getBounds());
    } catch (e) {
        console.error("Error fitting bounds:", e);
        map.setView([-7.0, 113.0], 10);
    }
    hideTable();
    applySearchFilter();
}

/* Function to show DAS map */
function showDASMap() {
    if (currentLayer) {
        map.removeLayer(currentLayer);
    }
    currentLayer = L.geoJSON(dasGeoJSON, {
        style: function (feature) {
            return {
                color: '#0288d1', // Consistent color as no 'class' property exists
                weight: 2,
                fillOpacity: 0.5
            };
        },
        onEachFeature: function (feature, layer) {
            var popupContent = `
                <div class="p-3" style="min-width: 200px; max-width: 300px; font-family: Arial, sans-serif; max-height: 300px; overflow-y: auto;">
                    <h5 class="mb-2 text-primary">${feature.properties.nama_das}</h5>
                    <table class="table table-sm table-bordered">
                        <tr><th class="bg-light">Nama DAS</th><td>${feature.properties.nama_das}</td></tr>
                        <tr><th class="bg-light">Kode DAS</th><td>${feature.properties.kode_das}</td></tr>
                        <tr><th class="bg-light">Luas DAS (mÂ²)</th><td>${feature.properties.luas_das.toLocaleString()}</td></tr>
                        <tr><th class="bg-light">Keliling (m)</th><td>${feature.properties.keliling_m.toLocaleString()}</td></tr>
                        <tr><th class="bg-light">Shape Length</th><td>${feature.properties.shape_Leng.toFixed(6)}</td></tr>
                        <tr><th class="bg-light">Shape Area</th><td>${feature.properties.shape_Area.toFixed(6)}</td></tr>
                        <tr><th class="bg-light">Metadata</th><td>${feature.properties.metadata}</td></tr>
                    </table>
                </div>
            `;
            layer.bindPopup(popupContent, { maxWidth: 300 });
        }
    }).addTo(map);
    try {
        map.fitBounds(currentLayer.getBounds());
    } catch (e) {
        console.error("Error fitting bounds:", e);
        map.setView([-7.0, 113.0], 10);
    }
    hideTable();
    applySearchFilter();
}  

 
// Function to update table dynamically with optional kabupaten filter
function updateTable(type, kabupatenFilter = null) {
    $('#tableContainer').show();
    $('#dataTable').empty();
    const groupedData = {};

    // Validasi data
    if (!facilitiesGeoJSON[type]) {
        console.error('Data untuk tipe ini tidak tersedia:', type);
        $('#tableContainer').hide();
        return;
    }

    // Group data and count facilities for the selected type
    facilitiesGeoJSON[type].forEach(item => {
        const key = `${type}_${item.kabupaten}_${item.provinsi || 'N/A'}`;
        if (!groupedData[key]) {
            groupedData[key] = {
                type: type,
                provinsi: item.provinsi || 'N/A',
                kabupaten: item.kabupaten,
                fasilitas: [],
                count: 0
            };
        }
        // Simpan nama fasilitas beserta deskripsinya
        groupedData[key].fasilitas.push({
            name: item.name,
            description: item.description || 'N/A' // Pastikan description diambil per item
        });
        groupedData[key].count++;
    });

    // Append table rows, applying kabupaten filter if provided
    Object.values(groupedData).forEach(group => {
        if (!kabupatenFilter || group.kabupaten.toLowerCase().trim() === kabupatenFilter.toLowerCase().trim()) {
            // Jika hanya satu fasilitas, buat satu baris
            if (group.fasilitas.length === 1) {
                $('#dataTable').append(`
                    <tr>
                        <td>${group.provinsi}</td>
                        <td>${group.kabupaten}</td>
                        <td class="fasilitas-cell">${group.fasilitas[0].name}</td>
                        <td>${group.fasilitas[0].description}</td>
                        <td>1</td>
                    </tr>
                `);
            } else {
                // Jika lebih dari satu fasilitas, buat baris terpisah untuk setiap fasilitas
                group.fasilitas.forEach(fasilitas => {
                    $('#dataTable').append(`
                        <tr>
                            <td>${group.provinsi}</td>
                            <td>${group.kabupaten}</td>
                            <td class="fasilitas-cell">${fasilitas.name}</td>
                            <td>${fasilitas.description}</td>
                            <td>1</td>
                        </tr>
                    `);
                });
            }
        }
    });

    // Calculate and append initial total
    updateTotal();
}

// Fungsi untuk memperbarui total berdasarkan baris yang terlihat
function updateTotal() {
    let totalCount = 0;
    $('#dataTable tr:visible').not('.total-row').each(function () {
        const countCell = $(this).find('td').eq(4).text();
        totalCount += parseInt(countCell) || 0;
    });

    // Hapus total row sebelumnya
    $('#dataTable .total-row').remove();
    
    // Tambahkan new total row
    $('#dataTable').append(`
        <tr class="total-row">
            <td colspan="4">Total</td>
            <td>${totalCount}</td>
        </tr>
    `);
}

// Fungsi lainnya tetap sama
function hideTable() {
    $('#tableContainer').hide();
    $('#toggleTableBtn').text('Tampilkan Tabel');
}

function toggleSidebar() {
    $('#sidebar').toggleClass('hidden');
    $('.toggle-btn i').toggleClass('bi-arrow-right bi-arrow-left');
    $('.table-container').css('left', $('#sidebar').hasClass('hidden') ? '20px' : '340px');
}

function toggleTable() {
    if ($('#tableContainer').is(':visible')) {
        $('#tableContainer').hide();
        $('#toggleTableBtn').text('Tampilkan Tabel');
    } else {
        $('#tableContainer').show();
        $('#toggleTableBtn').text('Sembunyikan Tabel');
    }
}

function applySearchFilter() {
    var value = $('#kabupatenSearch').val().toLowerCase().trim();
    if (!currentLayer) return;

    if (currentLayer instanceof L.GeoJSON) {
        let bounds = null;
        currentLayer.eachLayer(function (layer) {
            var kabupatenName = layer.feature?.properties?.Kabupaten?.toLowerCase() || '';
            if (value === '' || kabupatenName.includes(value)) {
                layer.addTo(map);
                if (!bounds) {
                    bounds = layer.getBounds();
                } else {
                    bounds.extend(layer.getBounds());
                }
            } else {
                map.removeLayer(layer);
            }
        });
        if (bounds) {
            try {
                map.fitBounds(bounds);
            } catch (e) {
                console.error("Error fitting bounds:", e);
                map.setView([-7.0, 113.0], 10);
            }
        } else {
            map.setView([-7.0, 113.0], 10);
        }
    } else if (currentLayer instanceof L.LayerGroup) {
        let bounds = null;
        currentLayer.eachLayer(function (layer) {
            var kabupatenName = layer.options?.kabupaten?.toLowerCase() || '';
            if (value === '' || kabupatenName.includes(value)) {
                layer.addTo(map);
                if (!bounds) {
                    bounds = L.latLngBounds([layer.getLatLng()]);
                } else {
                    bounds.extend(layer.getLatLng());
                }
            } else {
                map.removeLayer(layer);
            }
        });
        if (bounds) {
            try {
                map.fitBounds(bounds);
            } catch (e) {
                console.error("Error fitting bounds:", e);
                map.setView([-7.0, 113.0], 10);
            }
        } else {
            map.setView([-7.0, 113.0], 10);
        }
    }
}
//filter tabel
function applyTableFilter() {
    var value = $('#tableFilter').val().toLowerCase().trim();
    $('#dataTable tr').not('.total-row').each(function () {
        var provinsiCell = $(this).find('td').eq(0).text().toLowerCase();
        var kabupatenCell = $(this).find('td').eq(1).text().toLowerCase();
        var kategoriCell = $(this).find('td').eq(3).text().toLowerCase(); 
        if (value === '' || provinsiCell.includes(value) || kabupatenCell.includes(value) || kategoriCell.includes(value)) {
            $(this).show();
        } else {
            $(this).hide();
        }
    });

    // Perbarui total setelah filter diterapkan
    updateTotal();
}

// Event listener untuk input pencarian peta
$('#kabupatenSearch').on('input', applySearchFilter);
$('#dasSearch').on('input', applySearchFilter);

// Event listener untuk input pencarian tabel
$('#tableFilter').on('input', applyTableFilter);

// Inisialisasi peta dengan peta administrasi
showAdminMap();
